{
  "name": "book-scraper-backend",
  "version": "1.0.0",
  "description": "Backend for Book Scraper Application using Node.js, Express, TypeScript, and Puppeteer",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "node --loader ts-node/esm src/index.ts",
    "postinstall": "puppeteer install"
  },
  "keywords": [
    "books",
    "scraper",
    "express",
    "puppeteer",
    "typescript"
  ],
  "author": "Mbafor",
  "license": "MIT",
  "dependencies": {
    "@sparticuz/chromium": "^138.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "puppeteer": "^24.9.0",
    "puppeteer-core": "^24.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/node": "^20.5.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
